<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="feeder_feed_index" path="/home/feed" methods="GET POST">
        <default key="_controller">FeederBundle:Feed:index</default>
    </route>

    <route id="feeder_feeder_add" path="/home/add_feeder" methods="GET POST">
        <default key="_controller">FeederBundle:Feeder:add</default>
    </route>

    <route id="feeder_feeder_list" path="/home/feeders" methods="GET POST">
        <default key="_controller">FeederBundle:Feeder:list</default>
    </route>

    <route id="feeder_feeder_edit" path="/home/feeder/edit/{feederId}" methods="GET POST">
        <default key="_controller">FeederBundle:Feeder:edit</default>
    </route>

    <route id="feeder_feeder_delete" path="/home/feeder/delete/{feederId}" methods="GET POST">
        <default key="_controller">FeederBundle:Feeder:delete</default>
    </route>

    <route id="feeder_feedstat_remove" path="/home/remove_stat" methods="GET POST">
        <default key="_controller">FeederBundle:Feedstat:remove</default>
    </route>



</routes>